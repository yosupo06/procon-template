{
    "tasks": [
        {
            "type": "process",
            "label": "supporter: test",
            "command": "supporter",
            "args": [
                "t",
                "${file}"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": {
                "kind": "test",
                "isDefault": true
            }
        },
        {
            "type": "process",
            "label": "supporter: test (opt)",
            "command": "supporter",
            "args": [
                "t",
                "${file}",
                "-O"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "test"
        },
        {
            "type": "process",
            "label": "supporter: submit",
            "command": "supporter",
            "args": [
                "s",
                "${file}"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "none",
            "problemMatcher": []
        },
        {
            "type": "process",
            "label": "supporter: build",
            "command": "supporter",
            "args": [
                "b",
                "${file}",
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
        }
    ],
    "version": "2.0.0"
}